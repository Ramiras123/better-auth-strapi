{
	"name": "better-auth-strapi",
	"version": "0.1.1",
	"description": "A Better Auth plugin to authenticate with Strapi and refreshToken",
	"author": "@douweapausma and @ramiras123",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js",
			"require": "./dist/client.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"better-auth": "^1.4.14",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	},
	"keywords": [
		"better-auth",
		"strapi",
		"authentication",
		"auth",
		"plugin"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ramiras123/better-auth-strapi"
	}
}